            "capture": {
                "range_high": "highest(history, 10, 'high')",
                "range_low": "lowest(history, 10, 'low')"
            },
            "timeout": 0
        },
        {
            "id": "TRIGGER",
            "conditions": [
                "close > vars.range_high",
                "volume > moving_avg(history, 20, 'volume') * 1.8"
            ],
            "capture": {},
            "timeout": 5
        }
    ],
    "execution": {
        "side": "BUY",
        "entry": "close",
        "sl": "vars.range_low",
        "tp": "close + (close - vars.range_low) * 2.0",
        "option_selection": "ATM_CALL"
    },
    "description": "BIGDOG BREAKOUT LONG"
